<h1>ログイン</h1>

<%= form_with(scope: :session, local: true) do |f| %>
  <div class="form-group">
	  <%= f.label :email, "メールアドレス " %>
	  <%= f.text_field :email, id:"session_email", class:"form-control" %>
　</div>　
  <div class="form-group">
	  <%= f.label :password, "パスワード" %>
	  <%= f.password_field :password, id:"session_password", class:"form-control" %>
  </div>　
　<div class="form-group">
      <%= f.submit "ログインする", class:"btn btn-primary" %>
　</div>
<% end %>
